{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "npm install",
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/createLinkCode.ts": {
      "runtime": "@vercel/node"
    },
    "api/linkDevice.ts": {
      "runtime": "@vercel/node"
    },
    "api/sendUpdate.ts": {
      "runtime": "@vercel/node"
    },
    "api/fetchState.ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
